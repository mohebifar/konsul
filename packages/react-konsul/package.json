{
  "name": "react-konsul",
  "version": "1.8.0",
  "description": "A react renderer for konsul",
  "main": "lib/index.js",
  "scripts": {
    "watch": "babel src --watch --out-dir lib --ignore node_modules",
    "build": "rm -rf ./lib/* && babel src -d lib --ignore node_modules",
    "lint": "semistandard",
    "flow": "flow check",
    "prepublish": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohebifar/konsul.git"
  },
  "keywords": [
    "react",
    "konsul",
    "react-renderer"
  ],
  "files": [
    "lib"
  ],
  "author": "Mohamad Mohbeifar",
  "license": "MIT",
  "semistandard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ],
    "ignore": [
      "lib",
      "examples"
    ]
  },
  "bugs": {
    "url": "https://github.com/mohebifar/konsul/issues"
  },
  "homepage": "https://github.com/mohebifar/konsul#readme",
  "dependencies": {
    "react": "16.0.0-alpha.3",
    "react-dom": "16.0.0-alpha.3"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "eslint": "^6.6.0",
    "eslint-plugin-flowtype": "^2.30.0",
    "flow-bin": "^0.42.0",
    "semistandard": "^9.2.1"
  }
}
